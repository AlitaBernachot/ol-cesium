{
  "name": "ol-cesium",
  "version": "2.3.0",
  "description": "OpenLayers Cesium integration library",
  "scripts": {
    "doc": "typedoc --out dist/api --theme minimal --readme none --hideGenerator --listInvalidSymbolLinks --toc none",
    "typecheck": "tsc --pretty",
    "lint": "npm run eslint && npm run typecheck",
    "eslint": "eslint *.js",
    "eslint-fix": "eslint *.js --fix",
    "build-examples": "cross-env NODE_ENV=production TARGET=examples webpack --progress --bail",
    "build-library": "cross-env NODE_ENV=production TARGET=library webpack --progress --bail",
    "build-library-debug": "cross-env NODE_ENV=development TARGET=library webpack --progress --bail",
    "serve": "cross-env DEV_SERVER=1 TARGET=examples webpack-dev-server --port 3000 --progress --watch --bail --debug"
  },
  "main": "./dist/olcesium.js",
  "module": "./src/index.library.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/openlayers/ol-cesium.git"
  },
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/openlayers/ol-cesium/issues"
  },
  "devDependencies": {
    "@types/cesium": "^1.47.3",
    "@types/ol": "^4.6.2",
    "@types/proj4": "^2.3.4",
    "ol": "5.2.0",
    "proj4": "2.4.4",
    "typedoc": "git://github.com/gberaudo/typedoc#typescript-3.1.x",
    "typescript": "^3.1.0-dev.20180820",
    "@babel/core": "7.1.0",
    "@babel/preset-env": "7.1.0",
    "@camptocamp/cesium": "1.49.0",
    "@mapbox/geojsonhint": "2.0.1",
    "babel-loader": "8.0.2",
    "copy-webpack-plugin": "4.5.2",
    "cross-env": "5.2.0",
    "eslint": "5.6.0",
    "eslint-config-openlayers": "11.0.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "fs-extra": "7.0.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "htmlparser2": "3.9.2",
    "jsdoc": "3.5.5",
    "ls": "0.2.1",
    "nomnom": "1.8.1",
    "temp": "0.8.3",
    "webpack": "^4.20.2",
    "webpack-cli": "3.1.1",
    "webpack-dev-server": "3.1.9",
    "webpack-merge": "4.1.4"
  }
}
